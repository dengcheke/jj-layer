{
    "name": "jj-layer",
    "version": "1.2.1",
    "description": "custom arcgis 2D layer, used to visualize the simulation process of some models, use threejs as render engine",
    "scripts": {
        "example": "cross-env NODE_ENV=development webpack-dev-server --config ./build/example.config.js",
        "build-example": "rimraf docs && cross-env NODE_ENV=production webpack --config ./build/example.config.js",
        "build": "npm run build-esm && npm run build-cjs && npm run build-worker && npm run copy",
        "build-esm": "rimraf ./dist/esm && rollup -c build/rollup.esm.js",
        "build-cjs": "rimraf ./dist/cjs && node build/rollup.cjs.js",
        "build-worker": "rimraf ./dist/workers && node build/build-worker.js && ncp ./dist/workers/cjj-worker.js ./static/workers/cjj-worker.js",
        "copy": "ncp ./src/types ./dist/types && ncp ./README.MD ./dist/README.MD"
    },
    "keywords": [
        "arcgis",
        "threejs",
        "webgl",
        "raster visualize",
        "vector field",
        "flow line",
        "simulation process"
    ],
    "files": [
        "dist"
    ],
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.js",
    "types": "./dist/types/index.d.ts",
    "license": "ISC",
    "repository": "https://github.com/dengcheke/jj-layer",
    "browserslist": [
        "chrome >= 85"
    ],
    "devDependencies": {
        "@babel/cli": "^7.13.0",
        "@babel/core": "^7.13.8",
        "@babel/plugin-transform-runtime": "^7.12.1",
        "@babel/preset-env": "^7.13.9",
        "@rollup/plugin-alias": "^3.1.2",
        "@rollup/plugin-babel": "^5.3.0",
        "@rollup/plugin-commonjs": "^19.0.0",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-node-resolve": "^13.0.0",
        "@rollup/plugin-url": "^6.0.0",
        "@vue/babel-plugin-transform-vue-jsx": "^1.2.1",
        "@vue/composition-api": "^1.0.0-rc.8",
        "axios": "^0.21.1",
        "babel-loader": "^8.2.2",
        "clean-webpack-plugin": "^3.0.0",
        "copy-webpack-plugin": "^8.1.1",
        "cross-env": "^7.0.3",
        "css-loader": "^5.1.1",
        "echarts": "^5.1.1",
        "element-ui": "^2.15.1",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^5.2.0",
        "less": "^4.1.1",
        "less-loader": "^8.0.0",
        "ncp": "^2.0.0",
        "rollup": "^2.47.0",
        "rollup-plugin-terser": "^7.0.2",
        "style-loader": "^2.0.0",
        "typescript": "^4.5.5",
        "url-loader": "^4.1.1",
        "vue": "^2.6.12",
        "vue-charts": "^0.2.1",
        "vue-echarts": "^6.0.0-rc.5",
        "vue-loader": "^15.9.6",
        "vue-loader-plugin": "^1.3.0",
        "vue-router": "^3.5.1",
        "vue-template-compiler": "^2.6.12",
        "webpack": "^5.24.2",
        "webpack-bundle-analyzer": "^4.4.0",
        "webpack-cli": "^3.3.12",
        "webpack-dev-server": "^3.11.2",
        "webpack-node-externals": "^2.5.2",
        "@types/arcgis-js-api": "4.18.0"
    },
    "peerDependencies": {
        "three": ">=0.135.0",
        "esri-loader": "^3.0.0"
    },
    "dependencies": {
        "@babel/runtime-corejs3": "^7.14.0",
        "core-js": "^3.9.1",
        "lodash": "^4.17.21"
    }
}
